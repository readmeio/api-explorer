{
  "name": "api-explorer",
  "description": "Open source components that make up ReadMe&#39;s API explorer",
  "version": "0.0.0",
  "scripts": {
    "test": "node scripts/update-example-swagger-files && eslint -f unix example --ext js --ext jsx && lerna run test --stream",
    "start": "node scripts/update-example-swagger-files && webpack-dev-server",
    "build": "./build.sh & lerna run build",
    "watch": "lerna run watch --stream",
    "deploy": "gh-pages --dotfiles -d example/dist",
    "publish": "lerna publish",
    "version": "auto-changelog -v $(node -e \"console.log('v' + require('./lerna.json').version)\") && git add CHANGELOG.md"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "author": "Dom Harrington <dom@harrington-mail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:readme/api-explorer.git"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "auto-changelog": "^1.10.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.0-beta.4",
    "css-loader": "^1.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.1.1",
    "lerna": "^3.14.1",
    "style-loader": "^0.23.1",
    "uglify-es": "^3.2.1",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "acorn": "^6.1.1",
    "babel-polyfill": "^6.26.0",
    "livereactload": "^4.0.0-beta.2",
    "prop-types": "^15.7.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^3.1.3",
    "swagger2openapi": "^5.3.1",
    "whatwg-fetch": "^3.0.0"
  }
}
