{
  "name": "@readme/api-explorer",
  "description": "UI components for the API explorer",
  "version": "0.3.1",
  "main": "dist/index.js",
  "dependencies": {
    "@readme/oas-extensions": "^0.2.2",
    "@readme/syntax-highlighter": "^0.2.1",
    "classnames": "^2.2.5",
    "fetch-har": "^2.0.0",
    "httpsnippet": "^1.16.5",
    "js-cookie": "^2.1.4",
    "lodash.kebabcase": "^4.1.1",
    "marked": "github:readmeio/marked",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-jsonschema-form": "^1.0.0",
    "react-waypoint": "^7.3.1"
  },
  "scripts": {
    "lint": "eslint -f unix . --ext js --ext jsx",
    "inspect": "jsinspect",
    "prettier": "prettier --list-different \"./**/**.{js,jsx}\"",
    "pretest": "npm run lint && npm run inspect && npm run prettier",
    "test": "jest --coverage",
    "watch": "webpack -w",
    "build": "webpack --config webpack.prod.js"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./lib/enzyme"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/form-components",
      "dist/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org",
    "access": "public"
  },
  "author": "Dom Harrington <dom@harrington-mail.com>",
  "license": "ISC",
  "repository": "https://github.com/readmeio/api-explorer/tree/master/packages/api-explorer",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.37",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "@babel/preset-react": "^7.0.0-beta.37",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.0.4",
    "babel-loader": "^8.0.0-beta.0",
    "circular-json": "^0.5.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^22.0.4",
    "jsinspect": "^0.12.6",
    "mocha": "^3.4.2",
    "node-fetch": "^2.0.0-alpha.9",
    "nyc": "^11.0.3",
    "prettier": "^1.6.1",
    "raf": "^3.4.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.0.0",
    "uglify-es": "^3.2.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  }
}
