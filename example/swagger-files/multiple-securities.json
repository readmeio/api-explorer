{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "Multiple Securities"
  },
  "servers": [
    {
      "url": "http://example.com"
    }
  ],
  "paths": {
    "/things": {
      "post": {
        "operationId": "things",
        "security": [
          {
            "oauth": ["write:things"]
          },
          {
            "apiKey": []
          }
        ],
        "summary": "or security",
        "responses": {
          "405": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/and-security": {
      "post": {
        "operationId": "andSecurity",
        "security": [
          {
            "oauth": ["write:things"],
            "apiKey": [],
            "basicAuth": []
          }
        ],
        "summary": "and security",
        "responses": {
          "405": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/and-or-security": {
      "post": {
        "operationId": "andOrSecurity",
        "security": [
          {
            "oauth": ["write:things"],
            "apiKey": []
          },
          {
            "oauthDiff": ["write:things"]
          }
        ],
        "summary": "and or security",
        "responses": {
          "405": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/or-security": {
      "post": {
        "operationId": "orSecurity",
        "security": [
          {
            "oauth": ["write:things"]
          },
          {
            "apiKey": []
          }
        ],
        "summary": "or security",
        "responses": {
          "405": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/single-auth": {
      "post": {
        "operationId": "singleAuth",
        "security": [
          {
            "apiKey": []
          }
        ],
        "summary": "one security for endpoint",
        "responses": {
          "405": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/no-auth": {
      "post": {
        "operationId": "noAuth",
        "summary": "no security needed",
        "responses": {
          "405": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/multiple-oauths": {
      "post": {
        "operationId": "multipleOauths",
        "security": [
          {
            "oauth": ["write:things", "read:things"]
          },
          {
            "oauthDiff": ["write:things", "read:things"]
          }
        ],
        "summary": "or security",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/multiple-combo-auths": {
      "post": {
        "operationId": "unsupportedSchemeInTheAnd",
        "security": [
          {
            "oauth": ["write:things", "read:things"],
            "basicAuth": []
          },
          {
            "oauthDiff": ["write:things", "read:things"]
          }
        ],
        "summary": "second and does not show security",
        "responses": {
          "405": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/unknown-auth-type": {
      "post": {
        "operationId": "unknownAuthType",
        "security": [
          {
            "basicAuth": []
          }
        ],
        "summary": "unknown auth type",
        "responses": {
          "405": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/unknown-scheme": {
      "post": {
        "operationId": "unknownScheme",
        "security": [
          {
            "nonExistentScheme": []
          }
        ],
        "summary": "this scheme doesnt exist",
        "responses": {
          "405": {
            "description": "Invalid input"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "oauth": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "http://example.com/oauth/dialog",
            "scopes": {
              "write:things": "Add things to your account"
            }
          }
        }
      },
      "oauthDiff": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "http://example.com/oauth/dialog",
            "scopes": {
              "write:things": "Add things to your account"
            }
          }
        }
      },
      "apiKey": {
        "type": "apiKey",
        "name": "apiKey",
        "in": "header"
      },
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    }
  },
  "x-explorer-enabled": true,
  "x-samples-enabled": true,
  "x-samples-languages": ["curl", "node", "ruby", "javascript", "python"]
}
