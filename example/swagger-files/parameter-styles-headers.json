    "paths": {
        "/nonExplodedSimpleHeaders": {
            "get": {
                "summary": "Demonstrate all non-exploded simple header styles",
                "operationId": "demonstrateHeaders",
                "parameters": [{
                    "name": "primitive",
                    "in": "header",
                    "description": "simple style primitive",
                    "required": true,
                    "style": "simple",
                    "explode": false,
                    "schema": {
                        "type": "string"
                    }
                }, {
                    "name": "array",
                    "in": "header",
                    "description": "simple style array",
                    "required": true,
                    "style": "simple",
                    "explode": false,
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                }, {
                    "name": "object",
                    "in": "header",
                    "description": "simple style object",
                    "required": true,
                    "style": "simple",
                    "explode": false,
                    "schema": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "description": {
                                "type": "string"
                            }
                        }
                    }
                }],
                "responses": {
                    "200": {
                        "description": "Successful response"
                    }
                }
            }
        },
        "/explodedSimpleHeaders": {
            "get": {
                "summary": "Demonstrate all exploded simple header styles",
                "operationId": "demonstrateHeaders",
                "parameters": [{
                    "name": "primitive",
                    "in": "header",
                    "description": "simple style primitive",
                    "required": true,
                    "style": "simple",
                    "explode": true,
                    "schema": {
                        "type": "string"
                    }
                }, {
                    "name": "array",
                    "in": "header",
                    "description": "simple style array",
                    "required": true,
                    "style": "simple",
                    "explode": true,
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                }, {
                    "name": "object",
                    "in": "header",
                    "description": "simple style object",
                    "required": true,
                    "style": "simple",
                    "explode": true,
                    "schema": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "description": {
                                "type": "string"
                            }
                        }
                    }
                }],
                "responses": {
                    "200": {
                        "description": "Successful response"
                    }
                }
            }
        }
