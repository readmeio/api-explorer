{
    "openapi": "3.0.0",
    "info": {
      "description": "This is a fork of the petstore example, changed to support all the variations of the 'style' parameter",
      "version": "1.0.0",
      "title": "Parameter Styles",
      "contact": {
        "email": "aaron@readme.io"
      },
      "license": {
        "name": "Apache 2.0",
        "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
      }
    },
    "paths": {
        "/pet/{matrix}/{label}": {
            "get": {
                "summary": "Finds Pets by status",
                "operationId": "findPetsByStatus",
                "parameters": [{
                    "name": "matrix",
                    "in": "path",
                    "description": "Matrix style list of strings",
                    "required": true,
                    "style": "matrix",
                    "explode": false,
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                }, {
                    "name": "label",
                    "in": "path",
                    "description": "Label style list of strings",
                    "required": true,
                    "style": "matrix",
                    "explode": false,
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                }],
                "responses": {
                    "200": {
                        "description": "Successful response"
                    }
                }
            }
        }
    },
    "components": {
      "requestBodies": {
      },
      "securitySchemes": {
      },
      "schemas": {
        "Pet": {
          "type": "object",
          "required": [
            "name",
            "photoUrls"
          ],
          "properties": {
            "id": {
              "type": "integer",
              "format": "int64"
            },
            "name": {
              "type": "string",
              "example": "doggie"
            },
            "photoUrls": {
              "type": "array",
              "xml": {
                "name": "photoUrl",
                "wrapped": true
              },
              "items": {
                "type": "string"
              }
            },
            "tags": {
              "type": "array",
              "xml": {
                "name": "tag",
                "wrapped": true
              }
            },
            "status": {
              "type": "string",
              "description": "pet status in the store",
              "enum": [
                "available",
                "pending",
                "sold"
              ]
            }
          }
        }
      }
    }
  }
